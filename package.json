{
	"name": "vue-form-generator",
	"version": "3.0.0-dev",
	"description": "A schema-based form generator component for Vue.js",
	"homepage": "https://github.com/icebob/vue-form-generator",
	"main": "dist/vfg.js",
	"scripts": {
		"prebuild": "npm run test",
		"build:core": "cross-env FULL_BUNDLE=false webpack --progress --config build/webpack.build.config.js --define process.env.FULL_BUNDLE=false --output-filename=vfg-core.js",
		"build:full": "cross-env FULL_BUNDLE=true  webpack --progress --config build/webpack.build.config.js --define process.env.FULL_BUNDLE=true",
		"build": "cross-env NODE_ENV=production npm run build:core && cross-env NODE_ENV=production npm run build:full",
		"dev": "webpack-dev-server --config build/webpack.dev.config.js --inline --hot",
		"coverage": "cross-env NODE_ENV=test nyc npm run mocha && nyc report --reporter=text-lcov | coveralls",
		"changelog": "conventional-changelog -i CHANGELOG.md -s",
		"changelog:full": "conventional-changelog -i CHANGELOG.md -s -r 0",
		"docs": "cd docs && gitbook serve",
		"mocha": "mocha-webpack --webpack-config build/webpack.test.config.js --require test/unit/setup.js test/unit/specs/**/*.spec.js",
		"unit": "cross-env NODE_ENV=test nyc npm run mocha",
		"ci": "cross-env NODE_ENV=test mocha-webpack --watch --webpack-config build/webpack.test.config.js --require test/unit/setup.js test/unit/specs/**/*.spec.js",
		"test": "npm run unit"
	},
	"keywords": [
		"vue",
		"vuejs",
		"form",
		"generator",
		"schema",
		"json"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/icebob/vue-form-generator.git"
	},
	"files": [
		"dist/vfg.js",
		"dist/vfg.css",
		"dist/vfg-core.js",
		"dist/vfg-core.css",
		"src"
	],
	"author": "Icebob",
	"license": "MIT",
	"devDependencies": {
		"@vue/test-utils": "1.0.0-beta.11",
		"ajv": "6.2.1",
		"autoprefixer": "8.1.0",
		"babel-core": "6.26.0",
		"babel-eslint": "8.2.2",
		"babel-loader": "7.1.3",
		"babel-plugin-istanbul": "4.1.5",
		"babel-plugin-lodash": "3.3.2",
		"babel-plugin-transform-runtime": "6.23.0",
		"babel-preset-env": "1.6.1",
		"babel-preset-stage-2": "6.24.1",
		"chai": "4.1.2",
		"chromedriver": "2.35.0",
		"cleave.js": "1.1.2",
		"conventional-changelog-cli": "1.3.16",
		"coveralls": "3.0.0",
		"cross-env": "5.1.3",
		"css-loader": "0.28.10",
		"eonasdan-bootstrap-datetimepicker": "4.17.47",
		"eslint": "4.18.2",
		"eslint-config-prettier": "2.9.0",
		"eslint-friendly-formatter": "3.0.0",
		"eslint-loader": "2.0.0",
		"eslint-plugin-import": "2.9.0",
		"eslint-plugin-node": "6.0.1",
		"eslint-plugin-prettier": "2.6.0",
		"eslint-plugin-vue": "4.3.0",
		"expect": "22.4.0",
		"extract-text-webpack-plugin": "3.0.2",
		"fakerator": "0.3.0",
		"fecha": "2.3.3",
		"file-loader": "1.1.11",
		"inject-loader": "3.0.1",
		"ion-rangeslider": "2.2.0",
		"jquery": "3.3.1",
		"jsdom": "11.6.2",
		"jsdom-global": "3.0.2",
		"lodash": "4.17.5",
		"lolex": "2.3.2",
		"mocha": "5.0.1",
		"mocha-generators": "2.0.0",
		"mocha-loader": "1.1.3",
		"mocha-webpack": "1.0.1",
		"nightwatch": "0.9.20",
		"node-sass": "4.7.2",
		"nouislider": "11.0.3",
		"nyc": "11.4.1",
		"pikaday": "1.7.0",
		"prettier": "1.11.1",
		"pug": "2.0.0-rc.4",
		"pug-loader": "2.3.0",
		"sass-loader": "6.0.7",
		"sinon": "4.4.2",
		"sinon-chai": "2.14.0",
		"spectrum-colorpicker": "1.8.0",
		"stats-webpack-plugin": "0.6.2",
		"url-loader": "1.0.1",
		"vue": "2.5.13",
		"vue-loader": "14.1.1",
		"vue-multiselect": "2.0.8",
		"vue-style-loader": "4.0.2",
		"vue-template-compiler": "2.5.13",
		"webpack": "3.11.0",
		"webpack-dev-middleware": "2.0.6",
		"webpack-dev-server": "2.11.1",
		"webpack-merge": "4.1.2",
		"webpack-node-externals": "1.6.0"
	},
	"dependencies": {},
	"browserslist": [
		"last 2 versions"
	],
	"nyc": {
		"include": [
			"src/**/*.(js|vue)"
		],
		"instrument": false,
		"sourceMap": false,
		"reporter": [
			"lcov",
			"text"
		],
		"report-dir": "./test/unit/coverage",
		"temp-directory": "./test/unit/coverage/.nyc_output"
	}
}
